{
  "window.menuBarVisibility": "toggle",
  "editor.fontFamily": "JetBrains Mono, Fira Code Medium",
  "editor.fontLigatures": true,
  "editor.fontWeight": "700",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "formattingToggle.affects": [
    "formatOnSave",
    "formatOnPaste"
  ],
  // "editor.lineHeight": 25,
  "editor.wordWrap": "on",
  "editor.semanticHighlighting.enabled": "configuredByTheme",
  "editor.tokenColorCustomizations": {
    "[Cobalt2]": {
      /*
        teal - #9effff
        dark pink - #ff628c
        light green - #80ffbb
        green - #a5ff90
        dark green - #3ad900
        light purple - #fb94ff
      */
      "comments": "#929292",
      "keywords": "#ff628c",
      "numbers": "#95fcc5",
      //"types": "#9effff",
      "textMateRules": [
        {
          "name": "[GO] - package and import keywords",
          "scope": [
            "keyword.package.go",
            "keyword.import.go"
          ],
          "settings": {
            "foreground": "#9effff"
          }
        },
        {
          "name": "[GO] - package name and func keyword",
          "scope": [
            "keyword.interface.go",
            "entity.name.package.go",
            "keyword.function.go"
          ],
          "settings": {
            "foreground": "#ff628c"
          }
        },
        {
          "name": "[GO] - ellipsis operator",
          "scope": [
            "keyword.operator.ellipsis.go"
          ],
          "settings": {
            "foreground": "#fff"
          }
        },
        {
          "name": "[PYTHON] - Control Flow",
          "scope": [
            "keyword.control.flow.python"
          ],
          "settings": {
            "foreground": "#ff628c"
          }
        },
        {
          "name": "[PYTHON] - def keyword",
          "scope": [
            "storage.type.function.python"
          ],
          "settings": {
            "foreground": "#ff628c"
          }
        },
        {
          "name": "italic",
          "scope": [
            "variable.parameter.function-call.python",
            "meta.function-call.generic.python",
            "meta.indexed-name.python",
            "storage.type.string.python",
            "meta.object-literal.key.js"
            // "meta.item-access.python",
            // "meta.member.access.python"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "name": "[PYTHON] - def keyword",
          "scope": [
            "meta.function-call.arguments.python"
          ],
          "settings": {
            "foreground": "#ff628c"
          }
        },
      ]
    },
    "textMateRules": [
      {
        "scope": [
          //following will be in italics (=Pacifico)
          "comment",
          "entity.name.type.class", //class names
          "keyword", //import, export, return…
          //"support.class.builtin.js", //String, Number, Boolean…, this, super
          "storage.modifier", //static keyword
          "storage.type.class.js", //class keyword
          "storage.type.function.js", // function keyword
          "storage.type.js", // Variable declarations
          "keyword.control.import.js", // Imports
          "keyword.control.from.js", // From-Keyword
          //"entity.name.type.js", // new … Expression
          "keyword.control.flow.js", // await
          "keyword.control.conditional.js", // if
          "keyword.control.loop.js", // for
          "keyword.operator.new.js" // new
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          //following will be excluded from italics (My theme (Monokai dark) has some defaults I don't want to be in italics)
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json",
          "entity.name.type.class.js"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.tabSize": 4,
  "editor.renderWhitespace": "all",
  "workbench.editor.highlightModifiedTabs": true,
  "sync.gist": "2506dc6503d951dccfc1f32a7145f053",
  "files.autoSave": "onWindowChange",
  "explorer.sortOrder": "type",
  "explorer.openEditors.visible": 1,
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "phase",
  "debug.showBreakpointsInOverviewRuler": true,
  "sync.forceUpload": true,
  //"editor.acceptSuggestionOnEnter": "off",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "window.zoomLevel": 0,
  "git.autofetch": true,
  "terminal.integrated.shell.linux": "/bin/fish",
  //"goOutliner.extendExplorerTab": true,
  "[json]": {
    //"editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[go]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true,
      "source.fixAll": false
    },
  },
  "explorer.confirmDragAndDrop": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideTabs": true,
  "zenMode.silentNotifications": true,
  "todo-tree.general.tags": [
    "TODO",
    "FIXME",
    "IMPORTANT",
    "DEBUG",
    "HACK",
    "REVIEW",
    "NOTE",
    "TEST",
    "ALERT"
  ],
  "todo-tree.highlights.defaultHighlight": {
    //"icon": "alert",
    "type": "text-and-comment",
    "foreground": "red",
    "background": "white",
    //"opacity": 50,
    "iconColour": "black"
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "checklist",
      "foreground": "black",
      "background": "yellow",
      "iconColour": "#FFD700",
      "rulerColour": "#FFD700"
    },
    "FIXME": {
      "icon": "flame",
      "foreground": "#FF8C00",
      "background": "transparent",
      "iconColour": "#FF8C00",
      "rulerColour": "#FF8C00"
    },
    "IMPORTANT": {
      "icon": "issue-opened",
      "foreground": "#7FFF00",
      "background": "transparent", //"#65fa56",
      "iconColour": "#7FFF00",
      "rulerColour": "#7FFF00"
    },
    "DEBUG": {
      "icon": "bug",
      "foreground": "#00FFFF",
      "background": "transparent",
      "iconColour": "#00FFFF",
      "rulerColour": "#00FFFF"
    },
    "TEST": {
      "icon": "beaker",
      "foreground": "#65fa56", //#FF00FF
      "background": "black", //FF6347
      "iconColour": "#FF00FF",
      "rulerColour": "#FF00FF"
    },
    "ALERT": {
      "icon": "alert",
      "foreground": "#FFD700",
      "background": "red",
      "iconColour": "red",
      "rulerColour": "red"
    }
  },
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "breadcrumbs.enabled": true,
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}${separator}Code",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 200,
  "editor.minimap.showSlider": "always",
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": true,
  "files.insertFinalNewline": false,
  "bookmarks.useWorkaroundForFormatters": true,
